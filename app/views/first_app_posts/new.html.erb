<h1> New First App Post</h1>

<%= form_with model: @first_app_post do |form| %>
  <% if form.object.errors.any? %>
    <div>
      <% form.object.errors.full_messages.each do |message| %>
      <div> <%= message %></div>
      <% end %>
    </div>
  <% end %>

  <div>
    <%= form.label :titl %>
    <%= form.text_field :titl %>
  </div>

  <div>
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>

  <%= form.button %>
<% end %>